# Example Terraform variables file
# Copy this file to terraform.tfvars and customize the values

# AWS Region for deployment
aws_region = "us-east-1"

# Environment name (dev, staging, prod)
environment = "dev"

# Database credentials (use strong passwords in production)
db_username = "forecastflow_admin"
db_password = "your-secure-password-here"  # Change this!

# Optional: Custom domain name (leave empty to use default CloudFront/ALB domains)
domain_name = ""